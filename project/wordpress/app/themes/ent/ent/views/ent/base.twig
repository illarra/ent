{% spaceless %}
<!doctype html>
<html {{ site.language_attributes }} class="no-js">
<head>
    <meta charset="{{ site.charset }}">
    <title>{% block head_title %}{{ wp_title }}{% endblock %}</title>
    <meta name="description" content="{{ site.description }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="pingback" href="{{ site.pingback_url }}">
    <link rel="stylesheet" href="{{ asset('main.css') }}">
    {% block head %}{% endblock %}
    {{ wp_head }}
</head>
<body>
    {% block body %}{% endblock %}
    <script src="{{ asset('vendors.js') }}"></script>
    <script src="{{ asset('main.js') }}"></script>
    <script>require('main.js');</script>
    {{ wp_footer }}
    {#
    {% if google_maps_api_key %}
        <script async defer src="https://maps.googleapis.com/maps/api/js?key={{ google_maps_api_key }}&callback=onGoogleMapsLoaded"></script>
        <script>
            function onGoogleMapsLoaded() {
                if (typeof initContactMap === "function") {
                    initContactMap();
                }
            }
        </script>
    {% endif %}
    #}
</body>
</html>
{% endspaceless %}
